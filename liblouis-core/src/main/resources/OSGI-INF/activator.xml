<?xml version="1.0" encoding="UTF-8"?>
<scr:component xmlns:scr="http://www.osgi.org/xmlns/scr/v1.1.0" activate="activate" deactivate="deactivate" name="Liblouis provider and table registry">
    <implementation class="org.daisy.pipeline.liblouis.LiblouisProvider"/>
    <scr:reference bind="addTableSet"
        unbind="removeTableSet"
        interface="org.daisy.pipeline.liblouis.LiblouisTableSet"
        cardinality="0..n"
        name="LiblouisTableSet"
        policy="dynamic"/>
    <scr:reference bind="addBinary"
        unbind="removeBinary"
        interface="org.daisy.pipeline.liblouis.Binary"
        cardinality="0..n"
        name="LiblouisBinary"
        policy="dynamic"/>
</scr:component>
